{
  "name": "wgsl-literal",
  "displayName": "WGSL Literal",
  "description": "Syntax highlighting for wgsl inside of JavaScript tagged template strings",
  "version": "0.0.3",
  "author": "Gianmarco Simone",
  "publisher": "ggsimm",
  "repository": {
    "url": "https://github.com/gsimone/vscode-wgsl-literal.git"
  },
  "bugs": {
    "url": "https://github.com/gsimone/vscode-wgsl-literal/issues"
  },
  "engines": {
    "vscode": "^1.15.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "shader",
    "wgsl-literal",
    "webgpu",
    "javascript",
    "wgsl",
    "template"
  ],
  "extensionDependencies": [
    "PolyMeilex.wgsl"
  ],
  "icon": "icon.png",
  "contributes": {
    "grammars": [
      {
        "injectTo": [
          "source.js",
          "source.js.jsx",
          "source.jsx",
          "source.ts",
          "source.tsx"
        ],
        "scopeName": "wgsl-literal",
        "path": "./syntaxes/wgsl-literal.json",
        "embeddedLanguages": {
          "source.wgsl": "wgsl"
        }
      }
    ]
  }
}
